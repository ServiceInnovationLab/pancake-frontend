    "react": "16.5.1",
    "react-async-script-loader": "0.3.0",
    "react-autocomplete": "1.8.1",
    "react-dom": "16.5.1",
    "react-icons": "3.0.5",
    "react-modal": "3.5.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.5",
    "react-scroll": "1.7.10",
